<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN" 
	"http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>
	<!-- <properties resource="ibatis_config.properties" /> -->
	<typeAliases>
		<typeAlias type="com.vastsoft.yingtaidicom.search.orgsearch.systems.his.brand.Zhibang.ver1.entity.Z_BAH" alias="Z_BAH" />
		<typeAlias type="com.vastsoft.yingtaidicom.search.orgsearch.systems.his.brand.Zhibang.ver1.entity.B_BASY" alias="B_BASY" />
		<typeAlias type="com.vastsoft.yingtaidicom.search.orgsearch.systems.his.brand.Zhibang.ver1.entity.M_BRJBXX" alias="M_BRJBXX" />
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="${driver}" />
				<property name="url" value="${url}" />
				<property name="username" value="${username}" />
				<property name="password" value="${password}" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="com/vastsoft/yingtaidicom/search/orgsearch/systems/his/brand/Zhibang/ver1/config/HisZhibangVer1Mapper.xml" />
	</mappers>
</configuration>
